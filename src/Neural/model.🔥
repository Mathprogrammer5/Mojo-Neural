# defines the model trait and structs conforming to it
import Neural.layers as layer

trait Model:
  
  fn __init__(inout self):
    ...

  fn add(inout self, lyr: layers.Layer):
    ...

struct Sequential(Model):

  var lyrs: DynamicVector[layer.Layer]

  fn __init__(inout self):
    self.lyrs = DynamicVector[layer.Layer]()
  
  fn add(inout self, lyr: layer.Layer):
    self.lyrs.append(lyr)

fn main():
  pass
